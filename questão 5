function inverterString(stringDeEntrada) {
    let resultado = "";
    for (let i = stringDeEntrada.length - 1; i >= 0; i--) {
        resultado += stringDeEntrada[i];
    }
    return resultado;
}

// Exemplo de uso
const minhaString = "Ol√°, mundo!";
const stringInvertida = inverterString(minhaString);
console.log(stringInvertida);
